{
  "functions": {
    "api/*.py": {
      "runtime": "python@3.11"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/main.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ]
}
